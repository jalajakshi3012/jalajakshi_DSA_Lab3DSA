package com.gl.bstlab;

public class PairFinder {
	
	BinarySearchTree bst;
	Integer sum;
	public BinarySearchTree getBst() {
		return bst;
	}
	public void setBst(BinarySearchTree bst) {
		this.bst = bst;
	}
	public Integer getSum() {
		return sum;
	}
	public void setSum(Integer sum) {
		this.sum = sum;
	}
	
	

}
