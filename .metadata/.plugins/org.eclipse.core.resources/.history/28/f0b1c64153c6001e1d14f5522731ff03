package com.gl.bstlab;

public class BSTUtils {
	
	public static BinarySearchTree sampleBST(int[] a)
	{
		BinarySearchTree bst=new BinarySearchTree();
		
		bst.insertNode(70);
		bst.insertNode(50);
		bst.insertNode(90);
		bst.insertNode(30);
		bst.insertNode(60);
		bst.insertNode(80);
		bst.insertNode(100);
		bst.insertNode(20);
		bst.insertNode(40);
		
		return bst;
	}

}
